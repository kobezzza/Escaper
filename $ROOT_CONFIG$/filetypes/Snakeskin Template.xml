<filetype binary="false" description="Snakeskin Template" name="Snakeskin">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="///" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="@=;end;extends;import;include;interface;placeholder;template" ignore_case="false" />
    <keywords2 keywords="+=;-&gt;;^=;apply;block;callBlock;const;eval;global;head;proto;super;~=" />
    <keywords3 keywords="$forEach;=&gt;;bem;callback;case;catch;data;decl;default;doWhilst;else;elseIf;final;finally;for;forEach;forIn;forever;if;parallel;repeat;series;set;switch;throw;try;unless;until;var;waterfall;when;while;whilst;with" />
    <keywords4 keywords="%fileName%;?;attr;call;cdata;cdn;comment;doctype;link;output;plain;return;script;setBEM;style;tag;void" />
  </highlighting>
  <extensionMap>
    <mapping ext="ss" />
  </extensionMap>
</filetype>